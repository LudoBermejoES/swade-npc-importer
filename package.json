{
  "name": "swade-npc-importer",
  "version": "0.4.0",
  "description": "Import stat blocks for SWADE",
  "author": "Arnon Ram",
  "license": "LGPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "build": "yarn update:module && rollup -c --configPlugin @rollup/plugin-typescript",
    "build:watch": "yarn clean && rollup -cw --configPlugin @rollup/plugin-typescript",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "lint:check": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "zip-module": "cd dist && zip -r ../module.zip ./",
    "update:module": "node bin/update-module.cjs",
    "prepare": "husky"
  },
  "devDependencies": {
    "@guanghechen/rollup-plugin-copy": "^6.0.7",
    "@playwright/test": "^1.46.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/fvtt-types": "github:League-of-Foundry-Developers/foundry-vtt-types#main",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "happy-dom": "^18.0.1",
    "husky": ">=6",
    "jsdom": "^26.1.0",
    "lint-staged": ">=10",
    "prettier": "^3.5.3",
    "rollup": "^4.44.1",
    "rollup-plugin-livereload": "^2.0.5",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnonram/swade-npc-importer.git"
  },
  "bugs": {
    "url": "https://github.com/arnonram/swade-npc-importer/issues"
  },
  "homepage": "https://github.com/arnonram/swade-npc-importer#readme",
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "yarn": "^1.22.22"
  }
}
